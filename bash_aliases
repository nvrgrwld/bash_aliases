##########
#
# .aliases      Default user's .aliases file
#
##########  GIT
alias gitshow="git show HEAD~"
alias gitgraph='git fetch && git log --graph --pretty=format:'"'"'%h: %s - (%ar - %an)'"'"' origin/orders-tracker'
alias gitfiles="git log --name-only"
alias githistory="history | grep git | grep -v history"
alias gitdiff="git diff HEAD^ HEAD"
##########  HISTORY
alias hg="history | grep"
alias hist="history"
alias hh="history"
##########  FOLDERS
alias start="/mnt/c/Windows/explorer.exe ."
alias explorer="/mnt/c/Windows/explorer.exe ."
alias tracker="cd /mnt/c/Users/xxxxxxxxxxxx/Dev/Projects/WMS/REI.WMS/admin-frontend"
alias latest="cd /mnt/c/Users/xxxxxxxxxxxx/Dev/Projects/WMS/REI.WMS/admin-frontend"
alias tradeshow="cd /mnt/c/Users/xxxxxxxxxxxx/Dev/Projects/Tradeshow\ App/TradeshowApp"
alias shortcuts="cat /home/xxxxx/.bash_aliases"
alias reporting="cd /mnt/c/Users/xxxxxxxxxxxx/Dev/Projects/Reporting\ 3.0/REnvent\ Reporting"
alias cddev="cd /mnt/c/Users/xxxxxxxxx/Dev"
##########  GENERAL SHORTCUTS
#alias cd="cd & ls -alF"
alias findthis="echo egrep -lir ; egrep -lir"
alias vibash="vi ~/.bash_aliases"
alias vialias="vi ~/.bash_aliases"
alias vialiases="vi ~/.bash_aliases"
alias rebash="source ~/.bash_aliases"
alias sourceb="source ~/.bash_aliases"
alias xx="exit"
alias cls="clear"
alias df="df -h"
alias root="sudo -i"
alias cp="cp -i"
alias md="mkdir"
alias mv="mv -i"
alias rd="rmdir"
alias dir="ls"
alias ll="ls -alF --group-directories-first"
alias llt="ls -talF"
alias lltime="ls -talF"
alias lldate="ls -talF"
alias up="cd .."
alias ..="cd .."









